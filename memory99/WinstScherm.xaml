<Window x:Class="memory99.WinstScherm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:memory99"
        mc:Ignorable="d"
        Title="WinstScherm" Height="854" Width="1440">
    <Grid Background="#fff8f5">
        <Grid.RowDefinitions>
            <RowDefinition Height="20*" />
            <RowDefinition Height="20*" />
            <RowDefinition Height="20*" />
            <RowDefinition Height="20*" />
        </Grid.RowDefinitions>
        <Label x:Name="Regel1" Grid.Row="0" Content="Winnaar" FontFamily="/Font/BreeSerif-Regular.otf#Bree Serif" Foreground="#bf4d4d" FontSize="128"/>
        <Label x:Name="Regel2" Grid.Row="1" Content="Winnaar" FontFamily="/Font/BreeSerif-Regular.otf#Bree Serif" Foreground="#bf4d4d" FontSize="128"/>
        <Label x:Name="Regel3" Grid.Row="2" Content="Winnaar" FontFamily="/Font/BreeSerif-Regular.otf#Bree Serif" Foreground="#bf4d4d" FontSize="128"/>
        <Label x:Name="Regel4" Grid.Row="3" Content="Winnaar" FontFamily="/Font/BreeSerif-Regular.otf#Bree Serif" Foreground="#bf4d4d" FontSize="128"/>
        <Image x:Name="logo" Source="images/logo.png" Width="180" Height="164" HorizontalAlignment="Right" VerticalAlignment="Top"/>
    </Grid>
</Window>
